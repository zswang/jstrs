{
  "name": "jstrs",
  "title": "String functions",
  "description": "String functions",
  "version": "0.0.1",
  "homepage": "http://github.com/zswang/jstrs",
  "main": "jstrs.js",
  "author": {
    "name": "zswang",
    "url": "http://weibo.com/zswang"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zswang/jstrs.git"
  },
  "keywords": [
    "string",
    "function",
    "utf-8",
    "unicode"
  ],
  "bugs": {
    "url": "https://github.com/zswang/jstrs/issues"
  },
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "mocha": "^2.0.1",
    "istanbul": "^0.3.17",
    "should": "^4.1.0",
    "uglify-js": "^2.4.15",
    "jdists": "^1.0.9",
    "jshint": "^2.5.8"
  },
  "scripts": {
    "_update_version": "jdists version.jdists",
    "_dist": "jdists src/jstrs.js -o jstrs.js -r debug,test,remove,safe",
    "_compress": "uglifyjs jstrs.js -o jstrs.min.js -p 5 -c -m",
    "example": "jdists example.jdists.js -o test/example.js -t example",
    "test": "istanbul cover --hook-run-in-context node_modules/mocha/bin/_mocha -- -R spec",
    "mocha": "npm run example && mocha",
    "dist": "npm run _update_version && npm run example && npm run _dist && npm run _compress && npm run test",
    "lint": "jshint src/*.js *.json"
  },
  "files": [
    "jstrs.js"
  ]
}